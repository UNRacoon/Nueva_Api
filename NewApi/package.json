{
	"name": "apollo-gateway",
	"version": "1.0.0",
	"main": "gateway.js",
	"scripts": {
		"build": "babel src -d dist --source-maps",
		"dev-averages": "nodemon src/services/averages/index.js --exec babel-node src/services/averages/index.js",
		"dev-users": "nodemon src/services/users/index.js --exec babel-node src/services/users/index.js",
		"dev-schedules": "nodemon src/services/schedules/index.js --exec babel-node src/services/schedules/index.js",
		"dev-survey": "nodemon src/services/survey/index.js --exec babel-node src/services/survey/index.js",
		"dev-classrooms": "nodemon src/services/classrooms/index.js --exec babel-node src/services/classrooms/index.js",
		"dev-forums": "nodemon src/services/forums/index.js --exec babel-node src/services/forums/index.js",
		"dev-academic-history": "nodemon src/services/academic_history/index.js --exec babel-node src/services/academic_history/index.js",
		"dev-gateway": "nodemon src/gateway.js --exec babel-node src/gateway.js",
		"dev": "concurrently \"npm:dev-*\"",
		"start-averages": "node dist/services/averages/index.js",
		"start-schedules": "node dist/services/schedules/index.js",
		"start-survey": "node dist/services/survey/index.js",
		"start-users": "node dist/services/users/index.js",
		"start-classroom": "node dist/services/classrooms/index.js",
		"start-forums": "node dist/services/forums/index.js",
		"start-academic-history": "node dist/services/academic_history/index.js",
		"start-gateway": "node dist/gateway.js",
		"start": "concurrently \"npm:start-*\"",
		"clean": "rimraf dist"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"devDependencies": {
		"@babel/cli": "^7.11.6",
		"@babel/core": "^7.11.6",
		"@babel/node": "^7.10.5",
		"@babel/preset-env": "^7.11.5",
		"concurrently": "^5.3.0",
		"nodemon": "^2.0.4",
		"rimraf": "^3.0.2"
	},
	"dependencies": {
		"@apollo/gateway": "^0.21.0",
		"apollo-server": "^2.18.1",
		"axios": "^0.20.0",
		"dotenv": "^8.2.0",
		"graphql": "^15.3.0",
		"moment-timezone": "^0.5.31"
	}
}
